    "setTimeout",
    "clearTimeout",
    "setInterval",
    "clearInterval",
    "setImmediate",
    "clearImmediate",
    "tick",
    "tock",
    "nextTick",
    "time"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "devDependencies": {
    "assume": "0.0.x",
    "istanbul": "0.3.x",
    "mocha": "2.0.x",
    "pre-commit": "0.0.x"
  }
}
